{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
  },
  "cSpell.words": [
    "CURDATE",
    "datetime",
    "dayofmonth",
    "hana",
    "havings",
    "libx",
    "luxon",
    "mariadb",
    "Millis",
    "mtxs",
    "nvarchar",
    "Rdbms",
    "TIDB",
    "TINYINT",
    "typeorm",
    "ulonglong",
    "Unlocalized",
    "varchar",
    "VCAP"
  ],
  "jest.autoRun": {},
  "jest.jestCommandLine": "./node_modules/.bin/jest --passWithNoTests --runInBand --forceExit"
}