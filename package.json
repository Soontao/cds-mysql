{
  "name": "cds-mysql",
  "version": "0.0.1",
  "main": "lib/index.js",
  "description": "SAP CAP cds mysql adapter",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.17.1",
    "@sap/cds-sql": "^1.24.1",
    "luxon": "^1.25.0",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@sap/cds": "^4.5.1",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.25.1",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.19.0",
    "express": "^4.17.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}