{
  "name": "cds-mysql",
  "version": "6.3.6",
  "main": "lib/index.js",
  "cds.main": "./index.cds",
  "description": "mysql database adapter for cds",
  "scripts": {
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "npm run install:dev",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@newdash/graphlib": "3.1.3",
    "@newdash/newdash": "5.21.4",
    "cds-internal-tool": "1.7.0",
    "colors": "1.4.0",
    "generic-pool": "3.9.0",
    "glob": "8.0.3",
    "luxon": "3.2.1",
    "mysql2": "2.3.3",
    "ts-mysql-parser": "0.2.0",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@types/generic-pool": "3.1.11",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.5",
    "@types/luxon": "3.2.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "axios": "1.2.2",
    "eslint": "8.31.0",
    "jest": "29.3.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
