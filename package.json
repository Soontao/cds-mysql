{
  "name": "cds-mysql",
  "version": "7.0.0",
  "main": "lib/index.js",
  "cds.main": "./index.cds",
  "description": "mysql database adapter for cds",
  "bin": {
    "cds-mysql-build": "./bin/build"
  },
  "scripts": {
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebench": "npm run install:dev",
    "prebuild": "npm run install:dev",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "build": "tsc",
    "bench": "node bench/benchmark.js",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "alpha-release": "npx standard-version --prerelease alpha --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "@newdash/graphlib": "3.1.4",
    "@newdash/newdash": "5.22.0",
    "cds-internal-tool": "1.7.4",
    "colors": "1.4.0",
    "generic-pool": "3.9.0",
    "glob": "10.3.1",
    "luxon": "3.3.0",
    "mysql2": "3.4.2",
    "ts-mysql-parser": "0.2.0",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@types/generic-pool": "3.1.11",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.2",
    "@types/luxon": "3.3.0",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "axios": "1.4.0",
    "benchmark": "^2.1.4",
    "eslint": "8.43.0",
    "jest": "29.5.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB"
  ]
}
