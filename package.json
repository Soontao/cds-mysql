{
  "name": "cds-mysql",
  "version": "5.9.9",
  "main": "lib/index.js",
  "bin": {
    "cds-mysql-deploy": "./bin/cds-mysql-deploy.js"
  },
  "cds.main": "./index.cds",
  "description": "SAP CAP adapter for mysql",
  "scripts": {
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "rimraf lib && npm run install:dev",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/graphlib": "^3.1.2",
    "@newdash/newdash": "^5.21.2",
    "cds-internal-tool": "^0.0.24",
    "colors": "^1.4.0",
    "generic-pool": "^3.8.2",
    "glob": "^7.2.3",
    "luxon": "^2.4.0",
    "mysql2": "^2.3.3",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@types/generic-pool": "^3.1.10",
    "@types/jest": "^27.5.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^16.11.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "commitlint": "^16.3.0",
    "eslint": "^8.16.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
