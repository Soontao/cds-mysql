{
  "name": "cds-mysql",
  "version": "5.9.7",
  "main": "lib/index.js",
  "bin": {
    "cds-mysql-deploy": "./bin/cds-mysql-deploy.js"
  },
  "description": "SAP CAP adapter for mysql",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/graphlib": "^3.1.1",
    "@newdash/newdash": "^5.21.1",
    "colors": "^1.4.0",
    "generic-pool": "^3.8.2",
    "glob": "^7.2.0",
    "luxon": "^2.3.1",
    "mysql2": "^2.3.3",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "0.2.45"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@types/generic-pool": "^3.1.10",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.0",
    "@types/node": "^16.11.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "commitlint": "^16.2.3",
    "eslint": "^8.11.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
