{
  "name": "cds-mysql",
  "version": "0.1.7",
  "main": "lib/index.js",
  "bin": {
    "cds-mysql-deploy": "./bin/cds-mysql-deploy.js"
  },
  "description": "SAP CAP adapter for mysql",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.17.1",
    "@sap/cds-sql": "^1.24.1",
    "colors": "^1.4.0",
    "generic-pool": "^3.7.2",
    "glob": "^7.1.6",
    "graphlib": "^2.1.8",
    "luxon": "^1.26.0",
    "mysql2": "^2.2.5",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@sap/cds": "4.4",
    "@types/generic-pool": "^3.1.9",
    "@types/graphlib": "^2.1.7",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.26.1",
    "@types/node": "^14.14.31",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "commitlint": "^11.0.0",
    "eslint": "^7.21.0",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
