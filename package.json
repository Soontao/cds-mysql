{
  "name": "cds-mysql",
  "version": "6.1.0",
  "main": "lib/index.js",
  "cds.main": "./index.cds",
  "description": "mysql database adapter for cds",
  "scripts": {
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "npm run install:dev",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@newdash/graphlib": "^3.1.3",
    "@newdash/newdash": "^5.21.3",
    "cds-internal-tool": "^1.4.1",
    "colors": "^1.4.0",
    "generic-pool": "^3.9.0",
    "glob": "^8.0.3",
    "luxon": "^3.0.3",
    "mysql2": "^2.3.3",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@types/generic-pool": "3.1.11",
    "@types/glob": "8.0.0",
    "@types/jest": "28.1.8",
    "@types/luxon": "3.0.2",
    "@types/node": "16.11.68",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "axios": "1.1.3",
    "eslint": "8.25.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "uuid": "9.0.0"
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
