{
  "name": "cds-mysql",
  "version": "5.4.1",
  "main": "lib/index.js",
  "bin": {
    "cds-mysql-deploy": "./bin/cds-mysql-deploy.js"
  },
  "description": "SAP CAP adapter for mysql",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.20.0",
    "@newdash/graphlib": "^3.0.5",
    "colors": "^1.4.0",
    "generic-pool": "^3.8.2",
    "glob": "^7.2.0",
    "luxon": "^2.0.2",
    "mysql2": "^2.3.0",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "0.2.34"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@types/generic-pool": "^3.1.10",
    "@types/graphlib": "^2.1.8",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^2.0.5",
    "@types/node": "^14.17.21",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "commitlint": "^13.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@sap/cds": ">=5.5 <=6",
    "express": "4.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
