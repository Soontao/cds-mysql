{
  "name": "cds-mysql",
  "version": "6.0.0",
  "main": "lib/index.js",
  "cds.main": "./index.cds",
  "description": "SAP CAP adapter for mysql",
  "scripts": {
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "npm run install:dev",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-mysql"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@newdash/graphlib": "^3.1.2",
    "@newdash/newdash": "^5.21.2",
    "cds-internal-tool": "^1.3.0",
    "colors": "^1.4.0",
    "generic-pool": "^3.8.2",
    "glob": "^8.0.3",
    "luxon": "^3.0.0",
    "mysql2": "^2.3.3",
    "ts-mysql-parser": "^0.2.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/generic-pool": "3.1.11",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.5",
    "@types/luxon": "2.3.2",
    "@types/node": "16.11.43",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "axios": "0.27.2",
    "eslint": "8.19.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "uuid": "8.3.2"
  },
  "license": "MIT",
  "keywords": [
    "CDS",
    "CAP",
    "MySQL",
    "MariaDB",
    "TiDB"
  ]
}
