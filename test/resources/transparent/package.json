{
  "name": "transparent-migration",
  "cds": {
    "requires": {
      "db": {
        "kind": "mysql",
        "tenant": {
          "deploy": {
            "transparent": true
          }
        },
        "csv": {
          "migrate": true
        }
      },
      "auth": {
        "kind": "mocked",
        "users": {
          "alice": {
            "tenant": "default"
          }
        }
      },
      "extensibility": true,
      "cds.xt.ModelProviderService": true,
      "cds.xt.ExtensibilityService": {
        "element-prefix": [
          "x_",
          "xx_",
          "z_",
          "zz_"
        ],
        "extension-allowlist": [
          {
            "for": [
              "test.int"
            ],
            "kind": "entity",
            "new-fields": 50
          }
        ]
      },
      "cds.xt.DeploymentService": true,
      "kinds": {
        "cds.xt.DeploymentService": {
          "model": "../../../mtxs/DeploymentService.cds"
        },
        "mysql": {
          "impl": "../../../src/index"
        }
      }
    }
  }
}